            targetSdkVersion="0.0.2"/>

    <module 
            icon="icon.png" 
            title="Brick Breaker"
            author="Exhibit Team"
            version="0.0.1">

        <inputs>
        </inputs>
        <requires-module>
        </requires-module>

    </module>

</manifest>
